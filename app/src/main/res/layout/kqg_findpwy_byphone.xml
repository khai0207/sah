<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:drop="http://schemas.android.com/apk/res-auto"
    android:layout_gravity="center"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    style="@style/w_h_full">
    <RelativeLayout
        android:background="@drawable/sdk_ui_bg"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        style="@style/dialog_kqg_findpwy_byephone">
        <ImageView
            android:layout_width="45dp"
            android:layout_height="14dp"
            android:layout_marginTop="13.5dp"
            android:src="@drawable/sdk_ui_game"
            android:layout_centerHorizontal="true"/>
        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="230dp"
            android:layout_height="15dp"
            android:layout_marginTop="13.5dp"
            android:src="@drawable/sdk_pwd_head"
            android:layout_centerHorizontal="true"/>
        <RelativeLayout
            android:layout_width="270.5dp"
            android:layout_height="121dp"
            android:layout_marginLeft="14dp"
            android:layout_marginTop="36dp">
            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/sdk_ui_select_bg"
                android:scaleY="5.86"/>
            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="270.5dp"
                android:layout_height="27dp"
                android:layout_marginTop="7dp"
                android:src="@drawable/sdk_ui_bg1"/>
            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="249.5dp"
                android:layout_height="23dp"
                android:layout_marginLeft="11dp"
                android:layout_marginTop="9dp"
                android:src="@drawable/sdk_ui_bg_enter1"/>
            <LinearLayout
                android:id="@+id/linearLayout1"
                android:layout_width="249.5dp"
                android:layout_height="23dp"
                android:layout_marginLeft="11dp"
                android:layout_marginTop="9dp">
                <TextView
                    android:textColor="#ff000000"
                    android:gravity="center"
                    android:id="@+id/textView1"
                    android:layout_width="50dp"
                    android:layout_height="match_parent"
                    android:text="@string/find_password_old_account"/>
                <EditText
                    android:textSize="@dimen/login_username_hint"
                    android:textColor="#ff000000"
                    android:textColorHint="#c3c3c8"
                    android:layout_gravity="center_vertical"
                    android:id="@+id/user_name"
                    android:background="@drawable/edittext_bg"
                    android:layout_width="170dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="7dp"
                    android:hint="@string/find_password_account_confirm_hint"
                    android:singleLine="true"
                    android:maxLength="21"
                    android:inputType="textFilter"/>
            </LinearLayout>
            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="249.5dp"
                android:layout_height="23dp"
                android:layout_marginLeft="11dp"
                android:layout_marginTop="36dp"
                android:src="@drawable/sdk_ui_bg_enter1"/>
            <LinearLayout
                android:layout_width="249.5dp"
                android:layout_height="23dp"
                android:layout_marginLeft="11dp"
                android:layout_marginTop="36dp">
                <TextView
                    android:textColor="#ff000000"
                    android:gravity="center"
                    android:id="@+id/textView8"
                    android:layout_width="50dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="7dp"
                    android:text="@string/find_password_phone_num"/>
                <EditText
                    android:textSize="@dimen/login_username_hint"
                    android:typeface="normal"
                    android:textColorHint="#c3c3c8"
                    android:layout_gravity="center_vertical"
                    android:id="@+id/enter_phone"
                    android:background="@drawable/edittext_bg"
                    android:layout_width="170dp"
                    android:layout_height="match_parent"
                    android:hint="@string/find_password_has_bind_phone"
                    android:singleLine="true"
                    android:maxLength="21"
                    android:inputType="text"/>
            </LinearLayout>
            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="249.5dp"
                android:layout_height="23dp"
                android:layout_marginLeft="11dp"
                android:layout_marginTop="63dp"
                android:src="@drawable/sdk_ui_bg_enter1"/>
            <LinearLayout
                android:layout_width="249.5dp"
                android:layout_height="23dp"
                android:layout_marginLeft="11dp"
                android:layout_marginTop="63dp">
                <TextView
                    android:textColor="#ff000000"
                    android:gravity="center"
                    android:id="@+id/textView2"
                    android:layout_width="50dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="7dp"
                    android:text="@string/find_password_new_psw"/>
                <EditText
                    android:textSize="@dimen/login_username_hint"
                    android:typeface="normal"
                    android:textColorHint="#c3c3c8"
                    android:layout_gravity="center_vertical"
                    android:id="@+id/new_password"
                    android:background="@drawable/edittext_bg"
                    android:layout_width="170dp"
                    android:layout_height="match_parent"
                    android:hint="@string/find_password_new_psw_hint"
                    android:singleLine="true"
                    android:maxLength="21"
                    android:inputType="textPassword"/>
            </LinearLayout>
            <RelativeLayout
                android:layout_width="176dp"
                android:layout_height="23dp"
                android:layout_marginLeft="11dp"
                android:layout_marginTop="90dp">
                <ImageView
                    android:id="@+id/imageView13"
                    android:layout_width="249.5dp"
                    android:layout_height="33dp"
                    android:src="@drawable/sdk_ui_bg_enter1"
                    android:scaleY="2"/>
            </RelativeLayout>
            <LinearLayout
                android:layout_width="256dp"
                android:layout_height="23dp"
                android:layout_marginLeft="11dp"
                android:layout_marginTop="90dp">
                <TextView
                    android:textColor="#ff000000"
                    android:gravity="center"
                    android:id="@+id/textView12"
                    android:layout_width="50dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="7dp"
                    android:text="@string/change_phone_show_code"/>
                <EditText
                    android:textSize="@dimen/login_username_hint"
                    android:typeface="normal"
                    android:textColorHint="#c3c3c8"
                    android:layout_gravity="center_vertical"
                    android:id="@+id/identfying_code"
                    android:background="@drawable/edittext_bg"
                    android:layout_width="118dp"
                    android:layout_height="match_parent"
                    android:hint="@string/find_password_code_text_hiny"
                    android:singleLine="true"
                    android:maxLength="21"
                    android:inputType="textFilter"/>
                <Button
                    android:textSize="@dimen/cerented_szie"
                    android:textColor="#ffffffff"
                    android:id="@+id/get_code"
                    android:background="@drawable/sdk_button_yellow"
                    android:layout_width="66dp"
                    android:layout_height="22dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="-3dp"
                    android:text="@string/find_password_get_code_des"/>
            </LinearLayout>
        </RelativeLayout>
        <Button
            android:id="@+id/pwd_close"
            android:background="@drawable/sdk_ui_back1"
            android:layout_width="16dp"
            android:layout_height="25.5dp"
            android:layout_marginLeft="109.5dp"
            android:layout_marginTop="6dp"
            android:layout_marginBottom="109.5dp"
            android:layout_alignParentTop="true"
            android:layout_marginStart="109.5dp"
            android:layout_marginEnd="7dp"
            android:layout_alignParentEnd="true"/>
        <Button
            android:textSize="@dimen/dialog_text_des_size"
            android:textColor="#ffffffff"
            android:id="@+id/find_password"
            android:background="@drawable/sdk_button_yellow"
            android:layout_width="82dp"
            android:layout_height="27.5dp"
            android:layout_marginLeft="109.5dp"
            android:layout_marginBottom="11.5dp"
            android:text="@string/find_password_button_text"
            android:layout_alignParentBottom="true"
            android:layout_alignParentStart="true"/>
    </RelativeLayout>
</RelativeLayout>
