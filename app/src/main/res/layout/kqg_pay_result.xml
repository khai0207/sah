<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:drop="http://schemas.android.com/apk/res-auto"
    android:layout_gravity="center"
    android:background="@null"
    style="@style/w_h_full">
    <RelativeLayout
        android:background="@drawable/sdk_ui_bg"
        android:layout_centerInParent="true"
        style="@style/dialog_certificationuser">
        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="45dp"
            android:layout_height="14dp"
            android:layout_marginTop="13.5dp"
            android:src="@drawable/sdk_ui_game"
            android:layout_centerHorizontal="true"/>
        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="230dp"
            android:layout_height="15dp"
            android:layout_marginTop="13.5dp"
            android:src="@drawable/sdk_pwd_head"
            android:layout_centerHorizontal="true"/>
        <Button
            android:id="@+id/pwd_close"
            android:background="@drawable/sdk_ui_back1"
            android:layout_width="16dp"
            android:layout_height="25.5dp"
            android:layout_marginTop="7.5dp"
            android:layout_marginRight="4.5dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"/>
        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/linearLayout1"
            android:background="@drawable/sdk_ui_select_bg"
            android:layout_width="295dp"
            android:layout_height="36dp"
            android:layout_marginTop="37dp"
            android:layout_centerHorizontal="true">
            <TextView
                android:textColor="#ff000000"
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="@string/pay_num_des"
                android:layout_weight="1"/>
            <TextView
                android:typeface="normal"
                android:textColor="#ffff8c00"
                android:id="@+id/order_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_weight="1"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/linearLayout2"
            android:background="@drawable/sdk_ui_select_bg"
            android:layout_width="295dp"
            android:layout_height="66dp"
            android:layout_marginTop="5dp"
            android:layout_below="@+id/linearLayout1"
            android:layout_centerHorizontal="true">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1">
                <TextView
                    android:textColor="#ff000000"
                    android:id="@+id/textView3"
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:text="@string/order_goods_des"/>
                <TextView
                    android:textColor="#ffff8c00"
                    android:id="@+id/goods"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1">
                <TextView
                    android:textColor="#ff000000"
                    android:id="@+id/textView5"
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:text="@string/order_provide_name"/>
                <TextView
                    android:textColor="#ffff8c00"
                    android:id="@+id/provider"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1">
                <TextView
                    android:textColor="#ff000000"
                    android:id="@+id/textView7"
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:text="@string/pay_order_cost_des"/>
                <TextView
                    android:textColor="#ffff8c00"
                    android:id="@+id/order_money"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1">
                <TextView
                    android:textColor="#ff000000"
                    android:id="@+id/textView9"
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:text="@string/order_finish_time"/>
                <TextView
                    android:textColor="#ffff8c00"
                    android:id="@+id/finish_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </LinearLayout>
        </LinearLayout>
        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="4dp"
            android:src="@drawable/sdk_ui_pay_success"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"/>
        <TextView
            android:textSize="@dimen/cerented_szie"
            android:textColor="#ffffff"
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="16dp"
            android:text="@string/pay_success"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"/>
    </RelativeLayout>
</RelativeLayout>
